{
  "name": "cinrad-x-app",
  "version": "1.0.0",
  "description": "Get started cinrad-x-app with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zsumore/cinrad-x-app.git"
  },
  "author": "zsumore <zsumore@163.com> (http://www.fs121.com)",
  "license": "Apache License 2.0",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.0",
    "history": "^4.3.0",
    "whatwg-fetch": "^1.0.0",

    "material-ui": "^0.15.0",
    "moment": "^2.15.1",

    "react": "^15.3.0",
    "react-dom": "^15.3.0",

    "react-router": "^2.8.0",
    "react-tap-event-plugin": "^1.0.0",

    "react-redux": "^4.4.0",
    "redux-thunk": "^2.1.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-runtime": "^6.11.0",

    "better-npm-run": "0.0.11",

    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "ip": "^1.1.0",
    

    "normalize.css": "^4.2.0",
    "autoprefixer": "^6.5.0",
    "cssnano": "^3.7.0",

    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "cheerio": "^0.20.0",
    "codecov": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^2.0.0",

    "eslint": "^3.6.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.0",

    "express": "^4.14.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.0",
    "phantomjs-prebuilt": "^2.1.0",
    "react-addons-test-utils": "^15.3.0",
    "redbox-react": "^1.3.0",
    "sinon": "^1.17.0",
    "sinon-chai": "^2.8.0",

    "rimraf": "^2.5.0",

    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.8.0",
    "webpack-hot-middleware": "^2.12.0",

    "react-hot-loader": "^3.0.0-beta.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.0",
    "postcss-loader": "^0.13.0",
